<!DOCTYPE html>
<!-- Temporary login page -->
<html>
    <head>
        <meta charset="utf-8">
        <link rel = "stylesheet" href="../bootstrap/css/bootstrap.min.css" type = "text/css"> </link>
        <link rel="stylesheet" href="../stylesheets/main.css" type = "text/css">
    </head>
    <body>
      <%- include ../partials/disclaimer.ejs %>
  		<%- include ../partials/navbar.ejs %>

      <div class="container text-center">
          <div class="row">
              <div class="col-sm-12 page-header">
                  <h3 style ="text-align: center; color: blue; ">SFSU Software Engineering Project, Fall 2017.  For Demonstration Only</h3>
              </div>
          </div>
          <div class="row">
              <div class="col-sm-12">
                  <h1 class="text-center">Software Engineering class SFSU<p>Fall 2017</p><p>Section 2</p><p>Team 5</p></h1>
              </div>
          </div>
          <div class="row">
              <div class="col-sm-12">

                  <!-- The following buttons/links will be moved to the navbar -->
                  <!-- Link to home page -->
                  <a href="../home"><button type="button" class="btn btn-primary">Home</button></a>
                  <!-- Link to about page -->
                  <a href="../about"><button type="button" class="btn btn-primary">About Page</button></a>
                  <!-- Link to logout -->
                  <a href="logout"><button type="button" class="btn btn-primary">Logout</button></a>
                  <!-- Link to signup page -->
                  <a href="signup"><button type="button" class="btn btn-primary">Signup</button></a>
                  <!-- Link to user dashboard page -->
                  <a href="dashboard"><button type="button" class="btn btn-primary">Dashboard</button></a>

              </div>
          </div>
          <div class="formContainer row">
              <div class="col-sm-6">
                  <form action="addListing" method="post" enctype="multipart/form-data" >
                      <div class="form-group">
                          <label for="street">Street:</label>
                          <input type="text" class="form-control input-sm" id="listing-address" placeholder="Address, Apt, Suite" name="address" required>
                      </div>
                      <div class="form-group">
                          <label for="city">City:</label>
                          <input type="text" class="form-control" id="listing-city" placeholder="City" name="city" required>
                      </div>
                      <div class="form-group">
                          <label for="state">State:</label>
                          <input type="text" class="form-control" id="listing-state" placeholder="State" name="state" required>
                      </div>
                      <div class="form-group">
                          <label for="zipcode">Zipcode:</label>
                          <input type="text" class="form-control" id="listing-zipcode" placeholder="Zipcode" name="zipcode" required>
                      </div>
                      <div class="form-group">
                          <label for="price">Price:</label>
                          <input type="text" class="form-control" id="listing-price" placeholder="Price" name="price" required>
                      </div>
                      <div class="form-group">
                          <label for="bedroom-count">Number of Bedrooms:</label>
                          <input type="text" class="form-control" id="listing-bedroom-count" placeholder="Number of Bedrooms" name="bedroom" required>
                      </div>
                      <div class="form-group">
                          <label for="bathroom-count">Number of Bathrooms:</label>
                          <input type="text" class="form-control" id="listing-bathroom-count" placeholder="Number of Bathrooms" name="bathroom" required>
                      </div>
                      <div class="form-group">
                          <label for="floor-size">Floor Size:</label>
                          <input type="text" class="form-control" id="listing-floor-size" placeholder="Floor Size" name="floor" required>
                      </div>
                      <div class="form-group">
                          <label for="pool">Pool:</label>
                          <input type="text" class="form-control" id="listing-pool" placeholder="Pool Yes/No" name="pool" required>
                      </div>
                      <div class="form-group">
                          <label for="ac">AC:</label>
                          <input type="text" class="form-control" id="listing-ac" placeholder="AC Yes/No" name="ac" required>
                      </div>
                      <div class="form-group">
                          <label for="heater">Heater:</label>
                          <input type="text" class="form-control" id="listing-heater" placeholder="Heater Yes/No" name="heater" required>
                      </div>
                      <div class="form-group">
                          <label for="parking">Parking:</label>
                          <input type="text" class="form-control" id="listing-parking" placeholder="Parking Yes/No" name="parking" required>
                      </div>
                      <div class="form-group">
                          <label for="exampleInputFile">Image input:</label>
                          <br>
                          <input type="file" class="form-control-file" id="inputFile" name="image" aria-describedby="fileHelp" required>
                          <small id="fileHelp" class="form-text text-muted">Choose Images</small>
                      </div>
                      <button type="submit" class="btn btn-default" id = "submitButton">Submit</button>
                  </form>
              </div>
          </div>
      </div>
        		<%- include ../partials/footer.ejs %>
    </body>
</html>

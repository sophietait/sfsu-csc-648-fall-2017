<a href="addListingPage"><button class="btn btn-default" id="add-listing">Add Listing</button></a>
<div class="container">
    <div class="row seller-listing">
        <% if(data != null){ %>
            <% for(var i=0; i<data.length;i++) { %>
                <div class="col-sm-6 col-md-4 col-lg-3 mt-4">
                    <div class="card">
                        <a href="deleteListing/<%= data[i].listing_id %>">
                            <i class="glyphicon glyphicon-remove listing-delete"></i>
                        </a>
                        <a href="../listing/<%= data[i].listing_id %>">
                            <img class="card-img-top" alt="IMAGE N/A" src="<%= data[i].thumbnail %>" />
                            <div class="card-block">
                                <h4 class="card-title mt-3">
                                    <%= data[i].address %>, <%= data[i].city  %>,
                                    <%= data[i].state %> <%= data[i].zipcode %>
                                </h4>
                                <div class="card-text">
                                    $<%= data[i].price %>
                                </div>
                            </div>
                        </a>
                        <div class="card-footer"><small>Date Listed: <%= data[i].posted_on %></small>
                            <a href="editListing/<%= data[i].listing_id %>"><button class="btn btn-secondary btn-sm">Edit</button></a>
                        </div>
                    </div>
                </div>
            <% } %>
        <% } %>
    </div>
</div>
